<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" value="false" />
        <setting name="lazyLoadingEnabled" value="false" />
        <setting name="aggressiveLazyLoading" value="false" />
        <setting name="multipleResultSetsEnabled" value="true" />
        <setting name="useColumnLabel" value="true" />
        <setting name="useGeneratedKeys" value="false" />
        <setting name="defaultExecutorType" value="SIMPLE" />
        <setting name="defaultStatementTimeout" value="25000" />
    </settings>

    <typeAliases>
        <typeAlias alias="TblAccessResource" type="com.jmr.model.TblAccessResource"/>
        <typeAlias alias="TblAccount" type="com.jmr.model.TblAccount"/>
        <typeAlias alias="TblAuthority" type="com.jmr.model.TblAuthority"/>
        <typeAlias alias="TblAuthorityResource" type="com.jmr.model.TblAuthorityResource"/>
        <typeAlias alias="TblIndicatorStatistics" type="com.jmr.model.TblIndicatorStatistics"/>
        <typeAlias alias="TblRole" type="com.jmr.model.TblRole"/>
        <typeAlias alias="TblRoleAuthority" type="com.jmr.model.TblRoleAuthority"/>
        <typeAlias alias="TblUserRole" type="com.jmr.model.TblUserRole"/>
    </typeAliases>
</configuration>